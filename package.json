		"@sanity/block-content-to-html": "^2.0.0",
		"@sanity/client": "^6.4.0",
		"@sanity/image-url": "^1.0.2",
		"dotenv": "^16.3.1",
		"email-validator": "^2.0.4",
		"es6-promise": "^4.2.8",
		"isomorphic-fetch": "^3.0.0",
		"jquery": "^3.7.0",
		"nunjucks": "^3.2.4",
		"object-fit-images": "^3.2.4",
