{
  "functions": {
    "api/preview/index.js": {
      "includeFiles": "functions/serverless/**"
    },
    "api/landing-preview/index.js": {
      "includeFiles": "functions/serverlessLandings/**"
    }
  },
  "rewrites": [{ "source": "/", "destination": "/home" }],
  "buildCommand": "yarn build && cd blog && yarn build && cd build && rm -f index.html 404.html robots.txt && cp -R ./* ../../_site/",
  "installCommand": "yarn && cd blog && yarn"
}
