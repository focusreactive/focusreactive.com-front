   {% if data.seo.ogTwitter.card %}
      <meta name="twitter:card" content="{{ data.seo.ogTwitter.card }}" />
   {% endif %}

   {% if data.seo.ogTwitter.title %}
      <meta name="twitter:title" content="{{ data.seo.ogTwitter.title }}" />
   {% elif data.seo.title %}
      <meta name="twitter:title" content="{{ data.seo.title }}" />
   {% endif %}

   {% if data.seo.ogTwitter.description %}
      <meta name="twitter:description" content="{{ data.seo.ogTwitter.description }}" />
