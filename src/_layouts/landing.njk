   {% if data.seo.ogTags.siteName %}
      <meta property="og:site_name" content="{{ data.seo.ogTags.siteName }}">
   {% endif %}

   {% if data.seo.ogTags.image %}
      <meta property="og:image" content="{{ data.seo.ogTags.image }}">
   {% endif %}

   {% if data.seo.description %}
      <meta name="description" content="{{ data.seo.description }}">
   {% endif %}

   {% if data.seo.keyWords %}
      <meta name="keywords" content="{{ data.keyWords }}">
   {% endif %}

   {% if data.seo.articleTags.publisher %}
      <meta name="article:publisher" content="{{ data.seo.articleTags.publisher }}" />
   {% endif %}

   {% if data.seo.articleTags.modifiedTime %}
      <meta name="article:modified_time" content="{{ data.seo.articleTags.modifiedTime }}" />
