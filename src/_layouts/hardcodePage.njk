<!DOCTYPE html>
<html lang="en">

<head>

  {% if eleventy.env.isServerless %}
    <meta http-equiv="Cache-control" content="no-cache">
  {% endif %}

  {% if mainPageData.seo.ogTags.locale %}
    <meta property="og:locale" content="{{ mainPageData.seo.ogTags.locale }}">
  {% endif %}

  {% if mainPageData.seo.ogTags.type %}
    <meta property="og:type" content="{{ mainPageData.seo.ogTags.type }}">
  {% endif %}

  {% if mainPageData.seo.ogTags.title %}
    <meta property="og:title" content="{{ mainPageData.seo.ogTags.title }}">
  {% elif mainPageData.seo.title %}
    <meta property="og:title" content="{{ mainPageData.seo.title }}">
  {% endif %}

  {% if mainPageData.seo.ogTags.description %}
    <meta property="og:description" content="{{ mainPageData.seo.ogTags.description }}">
  {% elif mainPageData.seo.description %}
    <meta property="og:description" content="{{ mainPageData.seo.description }}">
  {% endif %}

  {% if mainPageData.seo.ogTags.url %}
    <meta property="og:url" content="{{ mainPageData.seo.ogTags.url }}">
  {% endif %}

  {% if mainPageData.seo.ogTags.siteName %}
    <meta property="og:site_name" content="{{ mainPageData.seo.ogTags.siteName }}">
  {% endif %}

  {% if mainPageData.seo.ogTags.image %}
    <meta property="og:image" content="{{ mainPageData.seo.ogTags.image }}">
  {% endif %}

  {% if mainPageData.seo.description %}
    <meta name="description" content="{{ mainPageData.seo.description }}">
  {% endif %}

  {% if mainPageData.seo.keyWords %}
    <meta name="keywords" content="{{ mainPageDatakeyWords }}">
  {% endif %}

  {% if mainPageData.seo.articleTags.publisher %}
    <meta name="article:publisher" content="{{ mainPageData.seo.articleTags.publisher }}" />
  {% endif %}

  {% if mainPageData.seo.articleTags.modifiedTime %}
    <meta name="article:modified_time" content="{{ mainPageData.seo.articleTags.modifiedTime }}" />
  {% endif %}

  {% if mainPageData.seo.ogTwitter.card %}
    <meta name="twitter:card" content="{{ mainPageData.seo.ogTwitter.card }}" />
  {% endif %}

  {% if mainPageData.seo.ogTwitter.title %}
    <meta name="twitter:title" content="{{ mainPageData.seo.ogTwitter.title }}" />
  {% elif mainPageData.seo.title %}
    <meta name="twitter:title" content="{{ mainPageData.seo.title }}" />
  {% endif %}

  {% if mainPageData.seo.ogTwitter.description %}
    <meta name="twitter:description" content="{{ mainPageData.seo.ogTwitter.description }}" />
  {% elif mainPageData.seo.description %}
    <meta name="twitter:description" content="{{ mainPageData.seo.description }}" />
  {% endif %}

  {% if mainPageData.seo.ogTwitter.image %}
    <meta name="twitter:image" content="{{ mainPageData.seo.ogTwitter.image }}" />
  {% endif %}

  {% if mainPageData.seo.ogTwitter.creator %}
    <meta name="twitter:creator" content="{{ mainPageData.seo.ogTwitter.creator }}" />
  {% endif %}

  {% if mainPageData.seo.ogTwitter.site %}
    <meta name="twitter:site" content="{{ mainPageData.seo.ogTwitter.site }}" />
  {% endif %}

  {% if mainPageData.seo.ogTwitter.label1 %}
    <meta name="twitter:label1" content="{{ mainPageData.seo.ogTwitter.label1 }}" />
  {% endif %}

  {% if mainPageData.seo.ogTwitter.data1 %}
    <meta name="twitter:data1" content="{{ mainPageData.seo.ogTwitter.data1 }}" />
  {% endif %}

  {% if mainPageData.seo.ogTwitter.label2 %}
     <meta name="twitter:label2" content="{{ mainPageData.seo.ogTwitter.label2 }}" />
  {% endif %}

  {% if mainPageData.seo.ogTwitter.data2 %}
     <meta name="twitter:data2" content="{{ mainPageData.seo.ogTwitter.data2 }}" />
  {% endif %}

  {% if mainPageData.seo.title %}
    <title>{{ mainPageData.seo.title }}</title>
  {% endif %}


  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <meta name="format-detection" content="telephone=no">
  <link href="/assets/css/app.css" rel="stylesheet" type="text/css">

  {% if eleventy.env.isServerless %}
  <script>
    window.addEventListener('message', receiveMessage, false);

    function receiveMessage(event) {
      if (event.origin === 'http://localhost:3333' || event.origin === 'https://fr-website.sanity.studio') {
        if(event.data === 'reload()') {
          window.location.reload();
        }
      }
      return;
    }
  </script>
  {% endif %}

  {% structuredData mainPageData %}

</head>

